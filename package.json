{
  "name": "bulma-skins-components",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/bulma-skins-components.umd.cjs",
  "module": "./dist/bulma-skins-components.js",
  "exports": {
    ".": {
      "import": "./dist/bulma-skins-components.js",
      "require": "./dist/bulma-skins-components.cjs"
    },
    "./src/css/animate.min.css": "./dist/css/animate.min.css",
    "./src/css/bulma-skins.min.css": "./dist/css/bulma-skins.min.css",
    "./src/css/bulma-skins.min.css.map": "./dist/css/bulma-skins.min.css.map",
    "./src/css/skins.js": "./dist/css/skins.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:translations": "node ./src/messages/runTranslations.js",
    "build": "vite build --config vite.config.js",
    "build:minify": "npm run build && uglifyjs ./dist/components.esm.js --compress --output ./dist/components.esm.min.js",
    "build:serve": "npm run build:minify && node index.js",
    "build:bulma": "sass --load-path=node_modules src/css/sass/bulma-skins.scss dist/style.css",
    "serve": "node index.js"
  },
  "dependencies": {
    "uuid": "^9.0.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.11.30",
    "@types/summernote": "^0.8.10",
    "@vitejs/plugin-vue": "^4.6.2",
    "bulma": "^1.0.0",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "uglify-js": "^3.17.4",
    "vite": "^4.5.3",
    "vue-tsc": "^1.8.27"
  }
}
